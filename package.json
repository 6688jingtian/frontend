{
  "name": "UIComponents",
  "version": "1.0.0",
  "description": "angular1.x UI components with ES6",
  "main": "app.js",
  "scripts": {
    "installPkgs": "npm install && npm install webpack -g && npm install npm-run-all -g && cd mockserver && npm install",
    "build": "webpack -d -s --progress",
    "dev": "webpack-dev-server",
    "prod": "webpack -p -d -s --progress --colors",
    "components": "webpack --config webpack.components.config.js",
    "componentsDev": "webpack-dev-server --config webpack.components.config.js",
    "componentsProd": "webpack --config webpack.components.config.js -p",
    "buildAll": "npm run build && npm run components",
    "buildAllProd": "npm run prod && npm run componentsProd",
    "start": "npm run buildAll && npm-run-all -p dev componentsDev"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bernardzhang/AngularWithES6.git"
  },
  "keywords": [
    "angular",
    "es6",
    "babel",
    "webpack",
    "spa"
  ],
  "author": "zhangyou",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bernardzhang/AngularWithES6/issues"
  },
  "homepage": "https://github.com/bernardzhang/AngularWithES6#readme",
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.23.1",
    "dedupe-plugin": "^1.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "filetransfer-webpack-plugin": "0.0.2",
    "happypack": "^3.0.2",
    "html-webpack-plugin": "^2.22.0",
    "html-webpack-template": "^5.0.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "minimist": "^1.2.0",
    "ng-annotate-loader": "^0.1.1",
    "npm-run-all": "^4.0.2",
    "raw-loader": "^0.5.1",
    "required-loader": "^1.0.10",
    "style-loader": "^0.13.1",
    "transfer-webpack-plugin": "^0.1.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-clean-plugin": "^0.2.3",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0",
    "webpack-node-externals": "^1.5.4",
    "webpack-uglify-parallel": "^0.1.3",
    "webpack-visualizer-plugin": "^0.1.6",
    "write-file-webpack-plugin": "^3.4.2"
  },
  "dependencies": {
    "angular": "1.5.8",
    "angular-animate": "1.5.8",
    "angular-aria": "1.5.8",
    "angular-material": "1.1.1",
    "angular-messages": "1.5.8",
    "angular-sanitize": "1.5.8",
    "angular-ui-router": "0.3.1",
    "angularjs-datetime-picker": "0.1.16",
    "babel-polyfill": "6.9.1",
    "bootstrap": "3.3.7",
    "d3": "^4.7.0",
    "echarts": "3.2.2",
    "jquery": "3.1.0",
    "moment": "2.14.1",
    "oclazyload": "1.0.9"
  }
}
